{
  "extends": [
    "config:recommended"
  ],
   "ignorePaths": [
    ".kokoro/requirements.txt"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["com.google.cloud:google-cloud-shared-config"],
      "registryUrls": ["https://repo1.maven.org/maven2"] // Add your desired repo URL here
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^.cloudbuild/*"
      ],
      "matchStrings": [
        "  _JAVA_SHARED_CONFIG_VERSION_TEMP: '(?<currentValue>.+?)'"
      ],
      "depNameTemplate": "com.google.cloud:google-cloud-shared-config",
      "datasourceTemplate": "maven"
    }
  ]
}
